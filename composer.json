{
    "name": "boo/address",
    "description": "Library to work with IPv4 and IPv6 addresses.",
    "keywords": [
        "ip",
        "ipv4",
        "ipv6",
        "address"
    ],
    "homepage": "https://github.com/jyggen/address",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas.stendahl@gmail.com",
            "homepage": "https://jyggen.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Boo\\Address\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boo\\Address\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
